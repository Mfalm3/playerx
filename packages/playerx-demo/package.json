  ],
  "scripts": {
    "build": "cross-env NODE_ENV=prod run-s css:prod js:prod eleventy",
    "dev": "cross-env NODE_ENV=dev run-p css:dev js:dev serve",
    "js:dev": "rollup -wc --silent",
    "js:prod": "rollup -c --silent",
    "css:prod": "postcss src/css/demo.css -o public/css/playerx-demo.css",
    "css:dev": "postcss src/css/demo.css -o public/css/playerx-demo.css -w",
    "lint": "eslint src/js",
    "serve": "eleventy --serve",
    "eleventy": "eleventy"
  },
  "dependencies": {
    "disco": "1.0.1",
    "playerx": "0.0.1",
