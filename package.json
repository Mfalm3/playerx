{
  "name": "playerx",
  "private": true,
  "version": "0.0.0",
  "description": "Playerx",
  "workspaces": [
    "packages/*"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint packages/*/src",
    "test": "cross-env COVERAGE=true karma start",
    "coverage": "codecov",
    "build": "rollup -c scripts/rollup.js --silent",
    "dev": "rollup -wc scripts/rollup.js --silent",
    "start": "http-server -c-1 -p 8000",
    "docs": "node scripts/docs.js && npm run format:docs",
    "format": "prettier --write '{packages,fixtures}/*/{src,test}/**/*.js' 'scripts/**/*.js' '**/*.md'",
    "format:docs": "prettier --write '**/*.md'"
  },
  "repository": "luwes/playerx",
  "keywords": [],
  "author": "Wesley Luyten <me@wesleyluyten.com> (https://wesleyluyten.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luwes/playerx/issues"
  },
  "homepage": "https://github.com/luwes/playerx#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@webcomponents/custom-elements": "^1.3.2",
    "ansi-colors": "^4.1.1",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "browserify": "^16.5.0",
    "codecov": "^3.6.5",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.20.1",
    "faucet": "^0.0.1",
    "fs-extra": "^8.1.0",
    "http-server": "^0.12.1",
    "ispy": "^0.1.2",
    "karma": "^4.4.1",
    "karma-browserstack-launcher": "^1.5.1",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage": "^2.0.1",
    "karma-rollup-preprocessor": "^7.0.3",
    "karma-sauce-launcher": "^1",
    "karma-tap": "^4.2.0",
    "karma-tap-pretty-reporter": "^4.2.0",
    "minimist": "^1.2.0",
    "prettier": "^1.16.4",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-gzip": "^2.3.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-size": "^0.2.2",
    "rollup-plugin-terser": "^5.2.0",
    "tap-spec": "^5.0.0",
    "tape-browser": "^4.13.0"
  }
}
